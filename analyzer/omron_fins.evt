import OMRON_FINS;
import Zeek_omron_fins;

protocol analyzer spicy::OMRON_FINS_UDP over UDP:
    parse with OMRON_FINS::Messages,
    ports {9600/udp};

export OMRON_FINS::Messages;
export OMRON_FINS::FINS_Header;
export OMRON_FINS::Command;

on OMRON_FINS::FINS_Header -> event OMRON_FINS::FINS_HeaderEvt (
    $conn,
    $is_orig,
    self
);

on OMRON_FINS::Command -> event OMRON_FINS::FINS_CommandEvt (
    $conn,
    $is_orig,
    self
);

